{
  "name": "@veksa/eslint-plugin-reselect-utils",
  "version": "5.1.0-p8",
  "description": "ESLint plugin to enforce best practices when using reselect and reselect-utils libraries",
  "homepage": "https://github.com/veksa/eslint-plugin-reselect-utils",
  "packageManager": "yarn@4.10.3",
  "author": {
    "name": "Aleksandr Khizhnyi",
    "email": "mendler88@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:veksa/eslint-plugin-reselect-utils.git"
  },
  "main": "./lib/cjs/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/cjs/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "lib/"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "reselect",
    "reselect-utils",
    "selector",
    "memoization"
  ],
  "scripts": {
    "compile": "tsc --noEmit",
    "test": "jest",
    "build": "shx rm -rf lib && tsc --outDir lib -m es2015 --declaration && tsc --outDir lib/cjs"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.44.1"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^8.44.1",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/parser": "^8.44.1",
    "@typescript-eslint/rule-tester": "^8.44.1",
    "@veksa/re-reselect": "^5.1.1-p8",
    "@veksa/reselect": "^5.1.1-p7",
    "@veksa/reselect-utils": "^5.1.0-p15",
    "common-tags": "^1.8.2",
    "eslint": "^9.36.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "shx": "^0.4.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.8.3"
  }
}
